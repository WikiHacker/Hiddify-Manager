{
	{% if 0 and hconfigs['vmess_enable'] and hconfigs['grpc_enable'] and hconfigs['h2_enable'] %}
		"inbounds": [
			{
				"tag": "h2-vmess-new",
				//"listen": "@@vmess-tcp-new-h2",
				"listen": "/opt/hiddify-manager/xray/run/vmesst.sock,666",
				"protocol": "vmess",
				"settings": {
				"clients": [
					{% for u in users %}
						{"id": "{{ u['uuid'] }}", "email": "{{ u['uuid'] }}@hiddify.com", "alterId": 0},
					{% endfor %}
				]
				},
				"streamSettings": {
				"network": "xhttp",
				"security": "none",
				"xhttpSettings": {
                    "mode": "stream-one",
					"path": "/{{ hconfigs['path_vmess'] }}{{ hconfigs['path_grpc'] }}",
				}
				
				},
				"sniffing": {
				"enabled": true,
				"destOverride": ["http","tls", "quic"]
				}
			}
		]
	{% endif %}
}